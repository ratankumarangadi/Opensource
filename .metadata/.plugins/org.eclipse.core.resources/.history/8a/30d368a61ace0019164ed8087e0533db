

public class Arrays {

	int[] array1;
	int [] array2;
	public void fillArray(){
		array1 = new int[10];
		array2= new int [10];
		for(int i=0; i<10; i++){
			array1[i]=i;
			array2[i] = i+1;
		}
		
		System.out.println(array1.length);
		System.out.println(array2.length);
		
	}
	
	public static void main(String[] args)
	{
	   Arrays a = new Arrays();
	   a.fillArray();
	}
}
